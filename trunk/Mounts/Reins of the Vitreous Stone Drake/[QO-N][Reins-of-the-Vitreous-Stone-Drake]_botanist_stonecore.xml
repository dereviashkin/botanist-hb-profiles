					<CustomBehavior File="Message" 
									Text="Exiting the instance so we can reset it."
									LogColor="Plum" />
					
					<!-- Move to - zone in location -->
					<MoveTo X="851.052" Y="986.474" Z="317.2723" />

					<!-- Go through the portal -->
					<CustomBehavior File="Misc\InInstance" 
