				<MoveTo X="1109.075" Y="877.5101" Z="286.6938" />
				
				<!-- Wait timer - wait 15 seconds - delay -->
				<CustomBehavior File="WaitTimer" 
								WaitTime="15000" />
				
				<!-- Move to - cave entrance -->
				<MoveTo X="1151.266" Y="931.5598" Z="284.5294" />
				
