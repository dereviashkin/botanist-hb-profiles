				<EnableBehavior Name="Combat" />
				
				<!-- Wait timer - wait 3 seconds - delay -->
				<CustomBehavior File="WaitTimer" 
								WaitTime="3000" />
				
				
				<!-- @@@@@@@@@@@@@@@@@@@@ 5th pull @@@@@@@@@@@@@@@@@@@@ -->
				
				
				<!-- Move to - pre-Slabhide -->
				<MoveTo X="1244.63" Y="1199.083" Z="247.5745" />
				
				<!-- Wait timer - wait 15 seconds - delay -->
				<CustomBehavior File="WaitTimer" 
								WaitTime="15000" />
				
				
				<!-- @@@@@@@@@@@@@@@@@@@@ 5th pull @@@@@@@@@@@@@@@@@@@@ -->
				
				
				<!-- Move to - Slabhide -->
				<MoveTo X="1292.352" Y="1226.478" Z="247.6368" />

				<!-- Wait timer - wait 15 seconds - delay -->
				<CustomBehavior File="WaitTimer" 
								WaitTime="15000" />

				<!-- Move to - Rock Borer kill zone -->
				<MoveTo X="1030.217" Y="871.4951" Z="297.1246" />				

				<!-- While our X > 900 -->
				<While Condition="Me.X &gt; 900">
					
						<!-- Custom behavior - TargetAndMoveToMob - Rock Borer -->
						<CustomBehavior File="TargetAndMoveToMob"
										MoveWithinMaxRangeOfMob="1"
										TargetOnlyIfHealthPercentAbove="5"
										WaitForNpcs="False"
										TerminateWhen="Me.GotTarget &amp;&amp; Me.CurrentTarget.IsDead"
										MobId="42845" >			
						</CustomBehavior>	<!-- Custom behavior - TargetAndMoveToMob -->

					<!-- If our target is a Rock Borer -->
