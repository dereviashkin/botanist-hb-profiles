			</If>	<!-- If we have less then 4 empty bag slots or our durability is less than 30% -->

			<!-- If our level >= 6 -->				
			<If Condition="Me.Level &gt;= 8" >
			
				<!-- Profile message  - We have reached level 8. Changing locations -->	
				<CustomBehavior File="Message" 
								Text="We have reached level 8. Changing locations." 
								LogColor="Cyan" />
								
				<!-- CustomBehavior - WaitTimer - 3 second delay -->
				<CustomBehavior File="WaitTimer" 
								WaitTime="3000" />				
								
			</If>	<!-- If our level >= 8 -->

		</While>	<!-- While loop - level >= 5 and < 6 -->


		<!-- If our level >= 6 -->				
		<If Condition="Me.Level &gt;= 6" >
		
			<!-- Profile message  - This portion has not been completed yet. Stopping the bot -->	
