			<CustomBehavior File="WaitTimer" 
							WaitTime="1000" />
			
			
			
			
			
			
			
			<!-- If we are not in Elwynn Forest -->
			<If Condition="Me.ZoneId != 12">
			
				<!--  CustomBehavior - LoadProfile - [Botanist][Tinker][A][hearthstone] -->
				<CustomBehavior File="LoadProfile" 
								ProfileName="Movement\[Botanist][Tinker]_unknown-to-elwynn-forest" />		
								
			</If>	<!-- If we are not in Elwynn Forest --> 
			
			<!-- If we are in Elwynn Forest and our hearthstone is not set to Goldshire -->
			<If Condition="Me.ZoneId == 12 &amp;&amp; Me.HearthstoneAreaId != 5637">
			
				<!--  CustomBehavior - LoadProfile - [Botanist][Tinker][A][hearthstone] -->
				<CustomBehavior File="LoadProfile" 
								ProfileName="Hearthstone\[Botanist][Tinker][A][hearthstone]" />
									
			</If>	<!-- If we are in Elwynn Forest and our hearthstone is not set to Goldshire -->
			
			<!-- If we are in Elwynn Forest and our hearthstone is set to Goldshire -->
			<If Condition="Me.ZoneId == 12 &amp;&amp; Me.HearthstoneAreaId == 5637">
			
				<!-- CustomBehavior - Message - Moving to the Tinker zone -->
				<CustomBehavior File="Message" 
								Text="Moving to the Tinker zone." 
								LogColor="Plum" />		
								
				<!-- Move to - 6 - 8 Tinker zone -->				
				<MoveTo X="-9521.241" Y="-526.3424" Z="62.97178" />
					
				<!-- CustomBehavior - Message - We have reached the Tinker zone. We will farm this area until we reach level 8 -->
				<CustomBehavior File="Message" 
								Text="We have reached the Tinker zone. We will farm this area until we reach level 8." 
								LogColor="Plum" />
									
			</If>	<!-- If we are in Elwynn Forest and our hearthstone is not set to Goldshire -->
			
			
			
			
			
			
			<!-- While loop - our empty bag slots > 4 and our lowest durability > 30 percent and our level < 8 -->				
			<While Condition="(Me.FreeBagSlots &gt; 4) &amp;&amp; (Me.LowestDurabilityPercent &gt; 0.3) &amp;&amp; (Me.Level &lt; 8)" >
				
				<!-- Custom behavior - TargetAndMoveToMob - Gray Forest Wolf -->
				<CustomBehavior File="TargetAndMoveToMob"
								MoveWithinMaxRangeOfMob="4"
								TargetOnlyIfHealthPercentAbove="5"
								WaitForNpcs="False"
								MobId="1922" >
					<HuntingGrounds>
						<Hotspot X="-9568.524" Y="-483.976" Z="61.87521" />
						<Hotspot X="-9280.763" Y="-639.2274" Z="64.82249" />
					</HuntingGrounds>			
				</CustomBehavior>	<!-- Custom behavior - TargetAndMoveToMob -->
			
				<!-- If our target is a Gray Forest Wolf -->
				<If Condition="(Me.CurrentTarget != null) &amp;&amp; (Me.CurrentTarget.Entry == 1922)">
				
					<!-- Profile message  - Engaging a Gray Forest Wolf -->	
					<CustomBehavior File="Message" 
									Text="Engaging a Gray Forest Wolf" 
									LogColor="Cyan" />
					
					<!-- Reset the instance -->
					<CustomBehavior File="RunMacro" 
									Macro="/startattack" />

				</If>	<!-- If our target is a Gray Forest Wolf --> 

			</While>	<!-- While loop - our empty bag slots > 4 and our lowest durability > 30 percent and our level < 6 -->
		
			<!-- If we have less then 4 empty bag slots or our durability is less than 30% -->
			<If Condition="(Me.FreeBagSlots &lt;= 4) || (Me.LowestDurabilityPercent &lt;= 0.3)" >
										
				<!-- Load profile - [Botanist][Tinker][1-58][repair-mail] -->
				<CustomBehavior File="LoadProfile" 
